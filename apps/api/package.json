{
  "name": "@qian-duo-duo/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc -p tsconfig.json",
    "generate-and-build": "npm run prisma:generate && npm run build",
    "start": "node dist/main.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier -w \"src/**/*.{ts,tsx,json}\"",
    "postinstall": "npx prisma generate --schema prisma/schema.prisma --config prisma.config.ts",
    "prisma:migrate": "npx prisma migrate dev --schema prisma/schema.prisma --config prisma.config.ts",
    "prisma:generate": "npx prisma generate --schema prisma/schema.prisma --config prisma.config.ts",
    "prisma:studio": "npx prisma studio"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.4",
    "@prisma/adapter-pg": "^6.19.0",
    "@prisma/client": "^7.1.0",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "fastify-type-provider-zod": "^6.1.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^24.10.2",
    "@types/pg": "^8.15.6",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "prisma": "^7.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
